<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileViewFragment4">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/more_score_more_reaches"
            android:id="@+id/head"
            android:textColor="#a100ff"
            android:textSize="18sp"
            android:textAlignment="center"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/more_your_scorewill_more_people_will_be_able_to_reach_you"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:id="@+id/subhead"
            android:textColor="#8C8984"
            android:layout_marginTop="5dp"
            android:textSize="15sp"
            android:textAlignment="center"/>
    </LinearLayout>

</FrameLayout>