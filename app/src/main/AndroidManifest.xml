<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pratham.hotornot_final">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fitsSystemWindows="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" android:screenOrientation="portrait"/>
        <activity android:name=".WallActivity" android:screenOrientation="portrait"/>
        <activity android:name=".LoginActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ImageRoller" android:screenOrientation="portrait"/>
        <activity android:name=".SettingsActivity" android:screenOrientation="portrait"/>
        <activity android:name=".AllPost" android:screenOrientation="portrait"/>
        <activity android:name=".CropActivity" android:screenOrientation="portrait"/>
        <activity android:name=".InformationActivity" android:screenOrientation="portrait"/>
        <activity android:name=".EditProfile" android:screenOrientation="portrait"/>
        <activity android:name=".ChangeValues"
            android:theme="@style/Theme.Transparent"
            android:screenOrientation="portrait"/>
        <activity android:name=".NewPassword" android:screenOrientation="portrait"/>
        <activity android:name=".LoginVerification" android:screenOrientation="portrait"/>
        <activity android:name=".NotificationSettings" android:screenOrientation="portrait"/>
        <activity android:name=".Support" android:screenOrientation="portrait"/>
        <activity android:name=".Privacy" android:screenOrientation="portrait"/>
        <activity android:name=".TermsAndCondition" android:screenOrientation="portrait"/>
    </application>

</manifest>